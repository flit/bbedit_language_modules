<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN"
"http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>BBEditDocumentType</key>
        <string>CodelessLanguageModule</string>
        <key>BBLMColorsSyntax</key>
        <true/>
        <key>BBLMIsCaseSensitive</key>
        <true/>
        <key>BBLMKeywords</key>
        <array>
            <dict>
                <key>RunKind</key>
                <string>com.barebones.bblm.preprocessor</string>
                <key>Keywords</key>
                <array>
                    <string>#define</string>
                    <string>#undef</string>
                    <string>#if</string>
                    <string>#ifdef</string>
                    <string>#ifndef</string>
                    <string>#else</string>
                    <string>#elif-</string>
                    <string>#endif</string>
                    <string>#include</string>
                </array>
            </dict>
            <dict>
                <key>RunKind</key>
                <string>com.barebones.bblm.keyword</string>
                <key>Keywords</key>
                <array>
                    <string>.macro</string>
                    <string>.endm</string>
                    <string>.macrolocal</string>
                    <string>.rodata</string>
                    <string>.text</string>
                    <string>.nothumb</string>
                    <string>.align</string>
                    <string>.type</string>
                    <string>.size</string>
                    <string>.data</string>
                    <string>.data.w</string>
                    <string>.data.h</string>
                    <string>.data.b</string>
                    <string>.endf</string>
                    <string>.file</string>
                    <string>.import</string>
                    <string>.global</string>
                    <string>.section</string>
                    <string>.string</string>
                    <string>.byte</string>
                    <string>.word</string>
                    <string>.space</string>
                    <string>.weak</string>
                    <string>.need</string>
                    <string>equ</string>
                    <string>.equ</string>
                    <string>.func</string>
                    <string>.endfunc</string>
                    <string>.end</string>
                    <string>.code</string>
                    <string>.cpu</string>
                    <string>.arch</string>
                    <string>.syntax</string>
                    <string>.thumb_func</string>
                    <string>.fpu</string>
                    <string>.eabi_attribute</string>
                    <string>.cfi_sections</string>
                    <string>.cfi_startproc</string>
                    <string>.cfi_endproc</string>
                    <string>.dcb</string>
                    <string>.dcb.w</string>
                    <string>.dcb.h</string>
                    <string>.dcb.l</string>
                    <string>.dcs</string>
                    <string>.ascii</string>
                    <string>.loc</string>
                    <string>.set</string>
                </array>
            </dict>
            <dict>
                <key>RunKind</key>
                <string>com.barebones.bblm.predefined-symbol</string>
                <key>Keywords</key>
                <array>
                    <string>TRUE</string>
                    <string>FALSE</string>
                    <string>bits</string>
                    <string>LOW</string>
                    <string>HIGH</string>
                    <string>Int</string>
                    <string>UInt</string>
                    <string>boolean</string>
                    <string>signal</string>
                    <string>integer</string>
                    <string>string</string>
                    <string>real</string>
                </array>
            </dict>
        </array>
        <key>BBLMLanguageCode</key>
        <string>ARM_</string>
        <key>BBLMLanguageDisplayName</key>
        <string>ARM Assembler</string>
        <key>BBLMScansFunctions</key>
        <true/>
        <key>BBLMSuffixMap</key>
        <array>
            <dict>
                <key>BBLMLanguageSuffix</key>
                <string>.arm</string>
            </dict>
        </array>
        <key>BBLMCanSpellCheckCodeRuns</key>
        <false/>
        <key>Language Features</key>
        <dict>
            <key>Identifier and Keyword Character Class</key>
            <string>a-zA-Z0-9_#.:</string>
            <key>Comment Pattern</key>
            <string>((//|;|@).*$)|(/\*(.|\r)*?\*/)</string>
            <key>String Pattern</key>
            <string>"(.|\\")*"</string>
            <key>Function Pattern</key>
            <string>^(?P&lt;function_name&gt;[a-zA-Z_][a-zA-Z_0-9:]*)</string>
        </dict>
    </dict>
</plist>
